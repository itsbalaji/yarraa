<div class="modal loginModal fade" id="loginModal" role="dialog" tabindex="-1" aria-labelledby="myModalLabel" ng-controller="LoginCtrl" name="login">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      
    <!--<div class="modal-header">
    	<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    	<h4 class="modal-title" id="myModalLabel">Yarraa Login</h4>
    </div>-->
      <div class="modal-body">
        <button type="button" title="Close" class="closeButton" data-dismiss="modal">
          <span aria-hidden="true">&times;</span>
          <span class="hide">Close</span>
        </button>
        <div id="yarraa-login" class="yarraa-login">
          <div class="login-content">
            <div class="login-form">
              <div class="login-form-header" align="center">
                <img src="img/signuplogo.jpg" class="img-responsive" alt="yarraa">
              </div>
              <form class="login-form-main" data-toggle="validator" role="form" name="loginForm" id="loginForm">
                <div class="form-group">
                  <input type="email" class="form-control" id="email" placeholder='{{"EMAIL"|translate}}' ng-model="newUser.email" required>
                </div>
                <div class="form-group">
                  <input type="password" class="form-control" id="password" placeholder='{{"PASSWORD"|translate}}' ng-model="newUser.pwd"  required>
                </div>		
                <div class="row">
                  <div class="col-xs-6">
                    <button class="btn btn-default btn-block" ng-click="login()"><i class="fa fa-check"></i> {{"SIGN_IN"|translate}}</button>
                </div>
                 <div class="col-xs-6" style="padding-top:10px; color:#5b3d19">
                  | &nbsp;&nbsp;<i class="fa fa-unlock-alt"></i>
                    <a class="login-form-reset" id="forgetpass" href="">{{"FORGOT_PASSWORD?"|translate}}</a>
                   </div>
                </div>
              </form>
              <div class="login-form-footer" style="padding:1rem; padding-top:0;">
                <h3 align="center" style="padding-top:0; padding-bottom:15px;">{{"DO_YOU_WANT_TO_CREAT"|translate}}</h3>
                <div class="row">
                  <div class="col-xs-6">
                    <button type="submit" class="btn btn-default btn-block signupTriggers"><i class="fa fa-user"></i> {{"SIGN_UP_AS_CONSUMER"|translate}}</button>
                  </div>
                  <div class="col-xs-6" align="right">
                    <button type="submit" class="btn btn-default btn-block signupTriggers"><i class="fa fa-group"></i> {{"SIGN_UP_AS_CORPORATE"|translate}}</button>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div> 
