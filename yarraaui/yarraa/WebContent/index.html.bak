<!-- Signup Modal -->
<div ng-controller="SignupCtrl" name="signup" ng-init="init()" class="modal signupModal fade" id="signupModal" role="dialog" tabindex="-1" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      
    <!--<div class="modal-header">
    	<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    	<h4 class="modal-title" id="myModalLabel">Yarraa Login</h4>
    </div>-->
      <div class="modal-body">
        <button type="button" title="Close" class="closeButton" data-dismiss="modal">
          <span aria-hidden="true">&times;</span>
          <span class="hide">{{"CLOSE"|translate}}</span>
        </button>
        <div class="row">
          <div class="col-xs-6">
            <div class="login-form-header" align="left" style="padding-top:0; padding-bottom:2rem">
              <img src="img/signuplogo.jpg" class="img-responsive" alt="yarraa">
            </div>
            <div><img src="img/cartimg.jpg"></img></div>
          </div>
          <div class="col-xs-6">
            <div id="yarraa-login" class="yarraa-login">
              <div class="login-content">
                <div class="login-form">
                 
                  <form class="login-form-main" data-toggle="validator" role="form" style="text-align:center;" name="signupForm" id="signupForm">
                     <h5 style="color:#5b3d19">Create your Yarraa account and start</h6>
                  <h4 style="color:#5b3d19; font-weight:bold;">managing all devices in 1 place!</h3>
                    <div class="form-group">
                      <input type="text" class="form-control" id="name" placeholder='{{"NAME"|translate}}' ng-model="newUser.first_name" required>
                    </div>
                    <div class="form-group">
                      <input type="email" class="form-control" id="email" placeholder='{{"EMAIL"|translate}}' ng-model="newUser.email" required>
                    </div>
                    <div class="form-group">
                      <input type="number" ng-pattern="onlyNumbers" class="form-control" name="telephone" id="telephone" 
                      		placeholder='{{"TELEPHONE"|translate}}'  ng-model="newUser.telephone" required>
                    </div>
                    <div class="form-group">
                      <select class="form-control" ng-model="newUser.country_code">
                      	<option disabled="disabled" selected value="">{{"COUNTRY"|translate}}</option>
                      	<option ng-repeat="option in countries" value="{{option.code}}">{{option.name}}</option>
                      </select>
                    </div>
                    <button class="btn btn-default btn-block" ng-click="saveRegistration()"><i class="fa fa-check"></i> {{"SIGN_UP_NOW"|translate}}</button>
                    
                  </form>
                 <div align="center"> <a href="javascript:void(0);" >{{"CREATE_COMP_ACC"|translate}}</a></div>
                 <div align="center">   <a href="javascript:void(0);"  class="loginTriggerClass">{{"ALREADY_HAVE_ACCOUNT"|translate}}</a></div>
                </div>
              </div>
            </div>
            
          </div>
        </div>
        
      </div>
    </div>
  </div>
</div> 
